# MATH FUNCTIONS AND VARS
fn add(a, b) {
a + b
}
fn sub(a, b) {
a - b
}
fn mul(a, b) {
a * b
}
fn div(a, b) {
a / b
}
fn mod(a, b) {
a % b
}
fn floor(a) {
a // 1
}
fn floordiv(a, b) {
a // b
}
fn ceil(a) {
a // 1 + 1
}
fn pow(a, b) {
a**b
}


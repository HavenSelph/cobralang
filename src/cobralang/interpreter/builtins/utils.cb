fn join(val, list) {
    let i = 0;
    let str = "";
    while (i < len(list)) {
        str += val + list[i];
        i += 1;
    }
    return str
}

fn split(val, str) {
    let list = [];
    let i = 0;
    while (i < len(str)) {
        list += str[i:i+len(val)];
        i += len(val);
    }
    return list
}
